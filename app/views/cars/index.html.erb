<h1>Car Listing</h1>

<table id="cars">
  <thead>
  <tr>
    <th>Make</th>
    <th>Model</th>
    <th>Year</th>
    <th>Price</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @car.each do |list| %>
    <tr>
      <td><%= list.make %></td>
      <td><%= list.model %></td>
      <td><%= list.year %></td>
      <td><%= list.price %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car', new_car_path %>

