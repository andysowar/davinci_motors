<h1>Car Listing</h1>

<table id="cars">
  <thead>
  <tr>
    <th>Year</th>
    <th>Make</th>
    <th>Model</th>
    <th>Price</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @cars.each do |car| %>
    <tr>
      <td><%= car.year %></td>
      <td><%= car.make %></td>
      <td><%= car.model %></td>
      <td><%= number_to_currency(car.price) %></td>
      <td><%= link_to 'Edit Car', edit_car_path(car) %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car', new_car_path %>


